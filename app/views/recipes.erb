<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <%@recipes.each do |recipe|%>
    <h1><%=recipe.name%></h1>
    <h3><%=recipe.cook_time%> minutes</h3>
    <h2><%=recipe.ingredients%></h2>
    <a href="/recipes/<%=recipe.id%>"></a>
    <%end%>
  </body>
</html>
