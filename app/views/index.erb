<!-- displays all of the recipes in the index -->
<h1>Welcome! Here are all of your recipes: </h1>

<ol>
<% @recipes.each do |recipe| %>
  <li>
    <a href="/recipes/<%= recipe.id %>">
      <%= recipe.name %>
    </a>
  </li>
<% end %>
</ol>


<a href="/recipes/new">Click here to add a new recipe</a>
